{
  "name": "render-gateway",
  "version": "0.0.3",
  "description": "The core implementation for a render server",
  "main": "dist/gateway/index.js",
  "repository": "git@github.com:Khan/render-gateway.git",
  "author": "Jeff Yates <jeff@khanacademy.org>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "KA_LOG_LEVEL=silly NODE_ENV=development babel-watch run_local.js",
    "clean": "rm -rf dist",
    "test": "jest --colors --config jest.config.js src",
    "coverage": "jest --colors --config jest.config.js --coverage",
    "lint:all": "yarn lint \"{run_local.js,src,bin,__{tests,mocks}__}/**/*.js\"",
    "lint": "eslint --report-unused-disable-directives --config .eslintrc.js --ignore-path .eslintignore",
    "flow:ci": "flow check",
    "build:noclean": "babel src --out-dir dist --source-maps --config-file ./.babelrc.js --ignore \"**/__tests__/**/*.js\",\"**/__mocks__/**/*.js\"",
    "build:flowdist": "node tools/build-flow-types.js",
    "build": "yarn clean && yarn build:noclean && yarn build:flowdist",
    "pretty": "prettier --config .prettierrc.js --ignore-path .prettierignore --write"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-flow": "^7.9.0",
    "@khanacademy/eslint-config": "^0.0.5",
    "ancesdir": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-watch": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-disable": "^2.0.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "flow-bin": "^0.122.0",
    "flow-typed": "^3.1.0",
    "husky": "^4.2.3",
    "jest": "^25.3.0",
    "jest-extended": "^0.11.5",
    "lint-staged": "^10.1.2",
    "prettier": "^2.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "yarn test --findRelatedTests",
      "yarn pretty"
    ]
  },
  "dependencies": {
    "@google-cloud/debug-agent": "^4.2.2",
    "@google-cloud/kms": "^2.0.0",
    "@google-cloud/logging-winston": "^3.0.6",
    "@google-cloud/profiler": "^3.0.1",
    "@google-cloud/trace-agent": "^4.2.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-winston": "^4.0.3",
    "superagent": "^5.2.2",
    "superagent-cache-plugin": "^2.1.0",
    "winston": "^3.2.1"
  },
  "resolutions": {
    "**/**/braces": "^2.3.1",
    "**/**/minimist": "^1.2.1"
  },
  "peerDependencies": {
    "jsdom": "^16.2.2"
  }
}
