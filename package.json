{
  "name": "render-gateway",
  "version": "0.0.2",
  "description": "The core implementation for a render server",
  "main": "dist/index.js",
  "repository": "git@github.com:Khan/render-gateway.git",
  "author": "Jeff Yates <jeff@khanacademy.org>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "KA_LOG_LEVEL=silly NODE_ENV=development babel-watch run_local.js",
    "clean": "rm -rf dist",
    "test": "jest --colors --config jest.config.js src",
    "coverage": "jest --colors --config jest.config.js --coverage",
    "lint": "eslint --report-unused-disable-directives --config .eslintrc.js \"{run_local.js,src,bin,__{tests,mocks}__}/**/*.js\"",
    "flow:ci": "flow check",
    "build:noclean": "babel src --out-dir dist --source-maps --config-file ./.babelrc.js --ignore \"**/__tests__/**/*.js\",\"**/__mocks__/**/*.js\"",
    "build": "yarn clean && yarn build:noclean",
    "pretty": "pretty-quick"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-flow": "^7.8.3",
    "@khanacademy/eslint-config": "^0.0.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-watch": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-disable": "^1.0.5",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "flow-bin": "^0.119.1",
    "flow-typed": "^3.0.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-extended": "^0.11.5",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "yarn test --findRelatedTests",
      "yarn pretty"
    ]
  },
  "dependencies": {
    "@google-cloud/debug-agent": "^4.2.2",
    "@google-cloud/kms": "^1.6.3",
    "@google-cloud/logging-winston": "^3.0.6",
    "@google-cloud/profiler": "^3.0.1",
    "@google-cloud/trace-agent": "^4.2.5",
    "express": "^4.17.1",
    "express-winston": "^4.0.3",
    "winston": "^3.2.1"
  }
}
